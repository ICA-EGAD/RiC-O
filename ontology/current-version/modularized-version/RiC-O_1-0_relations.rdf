         <owl:Restriction>
            <owl:onProperty rdf:resource="https://www.ica.org/standards/RiC/ontology#relation_role"/>
            <owl:hasSelf rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasSelf>
         </owl:Restriction>
      </owl:equivalentClass>
