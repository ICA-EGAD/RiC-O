   <!-- https://www.ica.org/standards/RiC/ontology#Group -->
   <owl:Class rdf:about="https://www.ica.org/standards/RiC/ontology#Group">
      <rdfs:subClassOf rdf:resource="https://www.ica.org/standards/RiC/ontology#Agent"/>
