                                          <rico:title xml:lang="fr">Radio Mayenne puis Radio France Mayenne</rico:title>
                                          <rdfs:label xml:lang="fr">Radio Mayenne puis Radio France Mayenne</rdfs:label>
                                          <rico:beginningDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1980-01-01</rico:beginningDate>
                                          <rico:endDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1986-12-31</rico:endDate>
