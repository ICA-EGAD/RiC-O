   <skos:Concept rdf:about="corporateBodyType/FRAN_RI_104-d-68iapa3gr--1778gsj0xqa96">
      <rdf:type rdf:resource="https://www.ica.org/standards/RiC/ontology#CorporateBodyType"/>
      <skos:inScheme rdf:resource="corporateBodyTypes"/>
      <skos:prefLabel xml:lang="fr">commission exécutive provisoire</skos:prefLabel>
      <skos:definition xml:lang="fr">Commission créée par l'Exécutif et chargée d'organiser une
         opération (événement) qui a une durée de vie éphémère. La commission est composée de
         commissaires généraux a qui est déléguée l'organisation de l'opération.</skos:definition>
      <skos:example>
         <rico:Agent rdf:about="agent/052119">
            <rdfs:label xml:lang="fr">France. Commission impériale de l'exposition universelle de
               1867 (1865-1872)</rdfs:label>
         </rico:Agent>
      </skos:example>
